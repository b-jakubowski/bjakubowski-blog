{
	"rulesDirectory": [
		"tslint-plugin-prettier"
	],
	"defaultSeverity": "error",
	"extends": [
		"tslint:latest",
		"tslint-react",
		"tslint-config-prettier"
	],
	"jsRules": {},
	"rules": {
		"interface-name": false,
		"prettier": true,
		"semicolon": [
			true,
			"never"
		],
		"member-access": false,
		"ordered-imports": true,
		"no-var-requires": true,
		"quotemark": [
			true,
			"single",
			"jsx-double"
		],
		"jsx-no-multiline-js": false,
		"jsx-no-lambda": false
	},
}